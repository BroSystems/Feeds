  },
  "homepage": "https://github.com/MoldaB/Feeds#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "express": "^4.16.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "mongodb": "^3.1.0-beta4",
    "morgan": "^1.9.0"
